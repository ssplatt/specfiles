-        $bl=$bl."|"."4".$_.$rrad.$_.$_nib;
-        $bl=$bl."|"."8".$_.$rrad.$_.$_oct;
-        $bl=$bl."|"."1".$_."0".$_.$rrad.$_.$_dec;
-        $bl=$bl."|"."1".$_."6".$_.$rrad.$_.$_hex;
